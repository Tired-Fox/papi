        <title>{project}</title>
        <python>
            def relative_url(origin, to="") -> str:
                origin = origin.split("/")
                to = to.split("/")
                if len(origin) < len(to):
                    start = len(to) - len(origin)
                    return '/'.join(to[start:]) + "/"
                else:
                    start = len(origin) - len(to)
                    return '/'.join('..' for _ in range(start)) + '/'.join(origin[start:])
        </python>
